<h1>Users</h1>

<%= link_to 'New', new_user_path %>

<table>
  <tr>
    <th>Email</th>
    <th>Id</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.id %></td>
      <td><%= link_to 'posts', user_posts_path(user) %></td>
    </tr>
  <% end %>
</table>
